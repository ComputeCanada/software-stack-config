{
    "NotebookNotary": {
        "db_file": ":memory:"
    },
    "FileManagerMixin": {
        "use_atomic_writing": false
    },
    "Lmod": {
        "launcher_pins": [
            "mate-websockify"
        ],
	"launcher_module_map": {
	    "fluentweb": "ansys/",
	    "fluent-websockify": "ansys/",
	    "cfx5-websockify": "ansys/",
	    "workbench-websockify": "ansys/",
	    "mapdl-websockify": "ansys/",
	    "paraview-websockify": "paraview",
	    "matlab-websockify": "matlab",
	    "starccm-websockify": "starccm,starccm-mixed",
	    "starccm-mixed-websockify": "starccm-mixed",
	    "comsol-websockify": "comsol",
	    "ansysedt-websockify": "ansysedt"
	}
    },
    "ServerProxy": {
        "servers": {
            "code-server": {
                "command": [
                    "code-server",
                    "--auth=none",
                    "--disable-telemetry",
                    "--socket={unix_socket}"
                ],
                "timeout": 180,
                "launcher_entry": {
                    "title": "VS Code",
                    "enabled": true,
                    "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/VSCode.svg"
                },
                "unix_socket": true
            },
            "openrefine": {
                "command": [
                    "refine",
                    "-i",
                    "127.0.0.1",
                    "-p",
                    "{port}",
                    "-H",
                    "*",
                    "-x",
                    "refine.headless=true"
                ],
                "timeout": 180,
                "launcher_entry": {
                    "title": "OpenRefine",
                    "enabled": true,
                    "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/OpenRefine.svg"
                }
            },
            "libreqda": {
                "command": [
                    "taguette",
                    "--no-browser",
                    "--base_path",
                    "{base_url}libreqda",
                    "--unix_socket",
                    "{unix_socket}",
                    "--debug"
                ],
                "unix_socket": true,
                "timeout": 180,
                "launcher_entry": {
                    "title": "LibreQDA",
                    "enabled": true,
                    "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/LibreQDA.svg"
                },
                "absolute_url": true
            },
            "mate-websockify": {
                "command": [
                    "/bin/sh",
                    "-c",
		    "cd && vncserver -rfbunixpath {unix_socket} -xstartup /home/mboisson/git/software-stack-config/jupyterhub_node/xstartup/mate -verbose -fg -geometry 1680x1050 -nolisten tcp -SecurityTypes None"
                ],
                "timeout": 300,
                "new_browser_window": true,
                "launcher_entry": {
                    "title": "Desktop (VNC)",
                    "path_info": "mate",
                    "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/Mate.svg"
                },
                "unix_socket": true,
                "raw_socket_proxy": true
            },
            "paraview-websockify": {
                "command": [
                    "/bin/sh",
                    "-c",
		    "cd && vncserver -rfbunixpath {unix_socket} -xstartup /home/mboisson/git/software-stack-config/jupyterhub_node/xstartup/paraview -verbose -fg -geometry 1680x1050 -nolisten tcp -SecurityTypes None"
