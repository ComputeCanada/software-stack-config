{
  "NotebookNotary": {
    "db_file": ":memory:"
  },
  "FileManagerMixin": {
    "use_atomic_writing": false
  },
  "Lmod": {
    "launcher_pins": [
      "desktop"
    ]
  },
  "ServerProxy": {
    "servers": {
      "code-server": {
        "command": [
          "code-server",
          "--auth=none",
          "--disable-telemetry",
          "--socket={unix_socket}"
        ],
        "timeout": 180,
        "launcher_entry": {
          "title": "VS Code",
          "enabled": true,
          "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/VSCode.svg"
        },
	"unix_socket": true
      },
      "openrefine": {
        "command": [
          "refine",
          "-i",
          "127.0.0.1",
          "-p",
          "{port}",
	  "-H",
	  "*",
          "-x",
          "refine.headless=true"
        ],
        "timeout": 180,
        "launcher_entry": {
          "title": "OpenRefine",
          "enabled": true,
	  "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/OpenRefine.svg"
        }
      },
      "libreqda": {
        "command": [
          "taguette",
          "--no-browser",
	  "--base_path",
	  "{base_url}libreqda",
	  "--unix_socket",
	  "{unix_socket}",
	  "--debug"
        ],
	"unix_socket": true,
        "timeout": 180,
        "launcher_entry": {
          "title": "LibreQDA",
          "enabled": true,
	  "icon_path": "/cvmfs/soft.computecanada.ca/config/jupyterhub_node/icons/libreqda.svg"
        },
	"absolute_url": true
      }
    }
  }
}
